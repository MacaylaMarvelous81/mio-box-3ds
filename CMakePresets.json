{
  "version": 10,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 13
  },
  "configurePresets": [
    {
      "name": "default",
      "displayName": "Default Config",
      "description": "Default build with devkitpro's cmake toolchain file and Ninja generator",
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/build/default",
      "cacheVariables": {
        "CMAKE_TOOLCHAIN_FILE": "$env{DEVKITPRO}/cmake/3DS.cmake"
      }
    }
  ]
}
